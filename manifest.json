{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action":  {
	  "default_title": "linkt2tab",
	  "default_popup": "settings.html"
  },
  "sidebar_action": {
	  "default_panel": "settings.html",
	  "open_at_install": true
  },
  "options_ui": {
	  "page": "settings.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+O"
      }
    }
  },
  "icons": {
	  "128": "icon.png"
  },
  "description": "Open Tabs with placeholder URLs and text selection",
  "manifest_version": 2,
  "name": "OpenWithSelection",
  "permissions": [
    "menus",
    "tabs",
    "storage"
  ],
  "version": "0.3.3"
}
